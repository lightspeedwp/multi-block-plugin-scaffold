{
	"name": "{{slug}}",
	"version": "{{version}}",
	"description": "{{description}}",
	"author": "{{author}}",
	"license": "{{license}}",
	"main": "build/index.js",
	"keywords": [
		"wordpress",
		"gutenberg",
		"block",
		"{{slug}}",
		"{{namespace}}"
	],
	"homepage": "https://github.com/{{author}}/{{slug}}",
	"repository": {
		"type": "git",
		"url": "https://github.com/{{author}}/{{slug}}.git"
	},
	"bugs": {
		"url": "https://github.com/{{author}}/{{slug}}/issues"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	},
	"scripts": {
		"build": "node bin/build.js",
		"build:quick": "wp-scripts build",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css:fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js:fix": "wp-scripts lint-js --fix",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint": "npm run lint:js && npm run lint:css && npm run lint:pkg-json",
		"lint:dry-run": "node bin/dry-run-test.js lint:js lint:css lint:pkg-json",
		"dry-run": "node bin/dry-run-test.js",
		"dry-run:lint": "node bin/dry-run-test.js lint:js lint:css",
		"dry-run:test": "node bin/dry-run-test.js test:unit",
		"dry-run:all": "node bin/dry-run-test.js lint:js lint:css test:unit",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"test:e2e": "wp-scripts test-e2e",
		"test:e2e:a11y": "wp-scripts test-e2e tests/e2e/accessibility.spec.js",
		"test:unit": "wp-scripts test-unit-js",
		"test:php": "phpunit",
		"test:scf": "phpunit tests/php/test-scf-json*.php",
		"test:scf:verbose": "phpunit tests/php/test-scf-json*.php --verbose",
		"validate:scf": "npm run test:scf -- --coverage-html coverage/scf/",
		"test": "npm run test:unit && npm run test:e2e",
		"lighthouse": "lhci autorun",
		"size-limit": "size-limit",
		"analyze-bundle": "webpack-bundle-analyzer build/stats.json build",
		"performance": "npm run lighthouse && npm run size-limit",
		"makepot": "wp i18n make-pot . languages/{{slug}}.pot --slug={{slug}} --domain={{textdomain}} --exclude=node_modules,vendor,build,tests",
		"makepot:php": "wp i18n make-pot . languages/{{slug}}.pot --slug={{slug}} --domain={{textdomain}} --exclude=node_modules,vendor,build,tests,src",
		"makejson": "wp i18n make-json languages --no-purge",
		"i18n": "npm run makepot && npm run makejson",
		"env": "wp-env",
		"env:start": "wp-env start",
		"env:stop": "wp-env stop",
		"env:restart": "wp-env start --update",
		"env:destroy": "wp-env destroy",
		"env:logs": "wp-env logs",
		"env:cli": "wp-env run cli wp",
		"update-version": "node bin/update-version.js"
	},
	"devDependencies": {
		"@lhci/cli": "^0.13.0",
		"@wordpress/babel-preset-default": "^8.35.0",
		"@wordpress/browserslist-config": "^6.35.0",
		"@wordpress/create-block": "^4.78.0",
		"@wordpress/e2e-test-utils-playwright": "^1.35.0",
		"@wordpress/env": "^10.35.0",
		"@wordpress/eslint-plugin": "^22.21.0",
		"@wordpress/jest-console": "^8.35.0",
		"@wordpress/jest-preset-default": "^12.35.0",
		"@wordpress/prettier-config": "^4.35.0",
		"@wordpress/scripts": "^31.0.0",
		"@wordpress/stylelint-config": "^23.27.0",
		"axe-core": "^4.10.2",
		"axe-playwright": "^2.0.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"size-limit": "^11.1.6",
		"webpack-bundle-analyzer": "^4.10.2"
	},
	"dependencies": {
		"@wordpress/a11y": "4.36.0",
		"@wordpress/api-fetch": "^7.35.0",
		"@wordpress/babel-plugin-makepot": "^6.35.0",
		"@wordpress/block-editor": "^15.8.0",
		"@wordpress/blocks": "^15.8.0",
		"@wordpress/components": "30.9.0",
		"@wordpress/core-data": "^7.35.0",
		"@wordpress/data": "^10.35.0",
		"@wordpress/date": "^5.35.0",
		"@wordpress/element": "^6.35.0",
		"@wordpress/i18n": "^6.8.0",
		"@wordpress/icons": "^11.3.0",
		"@wordpress/postcss-plugins-preset": "^5.35.0",
		"@wordpress/primitives": "^4.35.0",
		"@wordpress/react-i18n": "^4.35.0",
		"@wordpress/redux-routine": "^5.35.0",
		"@wordpress/style-engine": "^2.35.0",
		"@wordpress/wordcount": "^4.35.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"wp-scripts lint-js --fix",
			"wp-scripts format"
		],
		"*.{css,scss,sass}": [
			"wp-scripts lint-style --fix"
		],
		"*.php": [
			"composer run lint:fix"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dryRun": {
		"enabled": true,
		"description": "Enables dry-run mode for testing with mustache variable substitution"
	},
	"wp-env": {
		"plugins": [
			"."
		],
		"themes": [
			"https://wordpress.org/latest.zip"
		]
	},
	"projectMeta": {
		"displayName": "{{displayName}}",
		"namespace": "{{namespace}}",
		"website": "{{website}}",
		"docs": "{{docsUrl}}",
		"support": "{{supportUrl}}",
		"changelog": "{{changelogUrl}}",
		"repositoryType": "git",
		"repositoryUrl": "https://github.com/{{author}}/{{slug}}.git",
		"created": "{{createdDate}}",
		"updated": "{{updatedDate}}"
	}
}
