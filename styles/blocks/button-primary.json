  "class_name": "{{namespace}}-button-primary",
  "style_data": {
    "color": {
      "background": "var:preset|color|primary",
      "text": "var:preset|color|contrast"
    },
    "spacing": {
      "padding": {
        "top": "1rem",
        "bottom": "1rem",
        "left": "1.5rem",
        "right": "1.5rem"
      }
    },
    "typography": {
      "fontFamily": "var:preset|font-family|heading",
      "fontSize": "var:preset|font-size|medium"
    }
  }
}
