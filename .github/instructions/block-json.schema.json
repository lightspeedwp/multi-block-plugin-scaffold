{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "WordPress Block JSON Schema",
  "type": "object",
  "properties": {
    "apiVersion": { "type": "integer", "enum": [2, 3] },
    "name": { "type": "string", "pattern": "^[a-z0-9-]+\/[a-z0-9-]+$" },
    "title": { "type": "string" },
    "category": { "type": "string" },
    "icon": { "type": ["string", "object"] },
    "description": { "type": "string" },
    "keywords": { "type": "array", "items": { "type": "string" } },
    "version": { "type": "string" },
    "textdomain": { "type": "string" },
    "attributes": { "type": "object" },
    "supports": { "type": ["object", "boolean"] },
    "styles": { "type": "array" },
    "example": { "type": "object" },
    "editorScript": { "type": ["string", "array"] },
    "script": { "type": ["string", "array"] },
    "editorStyle": { "type": ["string", "array"] },
    "style": { "type": ["string", "array"] },
    "render": { "type": "string" },
    "usesContext": { "type": "array", "items": { "type": "string" } },
    "providesContext": { "type": "object" },
    "parent": { "type": ["string", "array"] },
    "ancestor": { "type": ["string", "array"] },
    "innerBlocks": { "type": "array", "items": { "type": "string" } },
    "blockHooks": { "type": "object" },
    "postTypes": { "type": "array", "items": { "type": "string" } },
    "variations": { "type": "array" },
    "deprecated": { "type": "array" },
    "provides": { "type": "object" },
    "requires": { "type": "object" },
    "$schema": { "type": "string" }
  },
  "required": ["apiVersion", "name", "title", "category", "icon", "description", "version", "textdomain"],
  "additionalProperties": true
}
